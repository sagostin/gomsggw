{
  "vhosts": [
    {
      "name": "rabbitmq"
    }
  ],
  "users": [
    {
      "name": "${RABBITMQ_DEFAULT_USER}",
      "password_hash": "",
      "password": "${RABBITMQ_DEFAULT_PASS}",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "${RABBITMQ_DEFAULT_USER}",
      "vhost": ".*",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ]
}